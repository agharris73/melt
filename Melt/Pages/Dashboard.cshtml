        <span class="small">Debt down to 43%.</span><br>
        <span class="small"><a href="#">Follow</a></span><br>
        <hr>
    </aside>
</div>

<script>
    AOS.init({
        offset: 100,
        duration: 600,
        easing: 'ease-in-sine',
        delay: 100,
        once: true,
    });
</script>

<script>
    $(function () {

        $("#range").ionRangeSlider({
            hide_min_max: true,
            keyboard: true,
            min: 0,
            max: 300000,
            from: 45000,
            to: 90000,
            type: 'double',
            step: 1,
            prefix: "$",
            grid: true
        });

    });
</script>

