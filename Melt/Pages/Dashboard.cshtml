@page
@model DashboardModel

<style>

    .overlay {
        position: absolute;
        width: 100%;
        height: 100%;
    }

    </style>

<script>

    $(document).ready(function () {

        @{
            Model.PercentToWorthlessness =  100;
            Model.Offset = ((@Model.PercentToWorthlessness * 3.8M) / 100) + 1;

            }

        $("#divClimber").parent().css({ position: 'relative' });
        $("#divClimber").css({ top: (108 * @Model.Offset) - 50, left: ((320 - ((85 * @Model.Offset * 0.70) - 55))) + 25, position: 'absolute' });

        //
    });

    $(function move() {
        $(".overlay").delay(20).animate({ top: top+10 }, function () {
            $(".overlay").delay(20).animate({ top: top }, move);
        });
    })
</script>

<div class="alert alert-warning alert-dismissible fade show" role="alert">
  <strong>Watch out below!</strong> Your challengers debt is melting! <a href="!#">Lead More</a>
  <button type="button" class="close" data-dismiss="alert" aria-label="Close">
    <span aria-hidden="true">&times;</span>
  </button>
</div>

<div class="wrapper">
    <header></header>

    <div id="divToast" style="display:none;">
        This is a div that should be shown.
    </div>
    <article class="main" style="margin-top:10px" data-aos="fade-down">
        <div id="divClimber" class="overlay">
            @if (Model.PercentToWorthlessness == 0)
            {
                <img src="~/images/winningClimber.png" width="40" />
            }
            else
            {
                <img src="~/images/climber.png" width="35" />
            }
        </div>
     </article>


    <!-- Page Content -->

    <aside class="aside aside-1">
        <div class="container">
            <div class="card" data-aos="fade-right">
                <div class="card-body">
                    <div class="user-image pb-1">
                        <img src="https://bootdey.com/img/Content/avatar/avatar7.png" width="50" class="img-radius"
                             alt="User-Profile-Image">
                    </div>
                    
                    <h5 class="card-title text-muted">Andrew Harris</h5>
                    <h6 class="card-subtitle mb-2 text-muted">Player One</h6>
                    <p class="card-text text-muted small">
                        Really wants to get his debt paid down so he can go fly
                        fishing in Costa Rica.
                    </p>

                    <div class="progress">
                        <div class="progress-bar" role="progressbar" aria-valuenow="70" aria-valuemin="0"
                             aria-valuemax="100" style="width: 70%">
                            70%
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="container pt-2">
            <div class="card" data-aos="fade-right">
                <div class="card-body">

                  <h5 class="card-title text-muted">Achievements</h5>
                    <img src="http://uxdrew.io/melt/achievement_on.png" class="original element-animation" width="25" >
                    <img src="http://uxdrew.io/melt/achievement_on.png" class="grayscale" width="25" >
                    <img src="http://uxdrew.io/melt/achievement_on.png" class="grayscale" width="25" >
                    <img src="http://uxdrew.io/melt/achievement_on.png" class="grayscale" width="25" >
                    <img src="http://uxdrew.io/melt/achievement_on.png" class="grayscale" width="25" >
                </div>
            </div>
        </div>

                <div class="container pt-2">
            <div class="card" data-aos="fade-right">
                <div class="card-body">

                  <h5 class="card-title text-muted">Predictive</h5>
                    <div class="slidecontainer">
  <input type="range" min="1" max="100" value="50" class="slider" id="myRange">
                        <p> <span id="slider_value" style="color:#555; text-align:left" class="small"></span></p>
</div>
                </div>
            </div>
        </div>

    </aside>
    <aside class="aside aside-2" data-aos="fade-left">
                <div class="container " data-aos="fade-right">
            <div class="card">
                <div class="card-body">
                    <div>
                                            <h6 class="card-subtitle mb-2 text-muted">Age Range</h6>


                        <input type="text" id="range3" value="" name="range" />

                    </div>
                </div>
            </div>
        </div>

                  <div class="container pt-2">
            <div class="card" data-aos="fade-right">
                <div class="card-body">
                    <div>
                    <h6 class="card-subtitle mb-2 text-muted">Marital Status</h6>
                    </div>
                </div>
            </div>
        </div>

                <div class="container pt-2">
            <div class="card" data-aos="fade-right">
                <div class="card-body">
                    <div>
                    <h6 class="card-subtitle mb-2 text-muted">Income</h6>
                    <input type="text" id="range" value="" name="range" />
                    </div>
                </div>
            </div>
        </div>

        <div class="container pt-2" data-aos="fade-right">
            <div class="card">
                <div class="card-body">
                    <div>
                        <h6 class="card-subtitle mb-2 text-muted">Education</h6>
                        <input type="text" id="range4" value="" name="range" />

                    </div>
                </div>
            </div>
        </div>

    </aside>
</div>


<script>
    AOS.init({
        offset: 100,
        duration: 600,
        easing: 'ease-in-sine',
        delay: 100,
        once: true,
    });
</script>

<script>
    $(function () {

        $("#range").ionRangeSlider({
            hide_min_max: true,
            keyboard: true,
            min: 0,
            max: 300000,
            from: 45000,
            to: 90000,
            type: 'double',
            step: 1,
            prefix: "$",
            grid: true
        });

    });
</script>

