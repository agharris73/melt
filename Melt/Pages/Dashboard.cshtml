@page
@model DashboardModel

<style>
    .overlay {
        position: absolute;
        width: 100%;
        height: 100%;
    }
</style>
<script>

    $(document).ready(function () {

        @*@{
            Model.PercentToWorthlessness = 100;
            Model.Offset = ((@Model.PercentToWorthlessness * 3.5M) / 100) + 1;
            }*@
        
        $("#divClimber").parent().css({ position: 'relative' });
        $("#divClimber").css({ top: (85 * @Model.Offset), left: ((280 - ((85 * @Model.Offset * 0.70) - 55))), position: 'absolute' });
    });

    $(function move() {
        $(".overlay").delay(20).animate({ top: top+10 }, function () {
            $(".overlay").delay(20).animate({ top: top }, move);
        });
    })
</script>
<div class="wrapper">
    <header></header>
    <article class="main" data-aos="fade-down">
        <div id="divClimber" class="overlay">
            <img src="~/images/climber.png" width="35" />
        </div>
    </article>


    <!-- Page Content -->

    <aside class="aside aside-1">
        <div class="container">
            <div class="card" data-aos="fade-right">
                <div class="card-body">
                    <div class="user-image pb-1">
                        <img src="https://bootdey.com/img/Content/avatar/avatar7.png" width="50" class="img-radius"
                             alt="User-Profile-Image">
                    </div>

                    <div class="user-image py-1 float-right">
                        <img src="http://uxdrew.io/melt/lunch_sack_3.svg" width="35" class="img-radius" alt="User-Profile-Image">
                        <img src="http://uxdrew.io/melt/lunch_sack_3.svg" width="35" class="img-radius" alt="User-Profile-Image">
                        <img src="http://uxdrew.io/melt/lunch_sack_3.svg" width="35" class="img-radius" alt="User-Profile-Image">

                    </div>

                    <h5 class="card-title text-muted">Andrew Harris</h5>
                    <h6 class="card-subtitle mb-2 text-muted">Player One</h6>
                    <p class="card-text text-muted small">
                        Really wants to get his debt paid down so he can go fly
                        fishing in Costa Rica.
                    </p>

                    <div class="progress">
                        <div class="progress-bar" role="progressbar" aria-valuenow="70" aria-valuemin="0"
                             aria-valuemax="100" style="width:70%">
                            70%
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="container pt-2">
            <div class="card" data-aos="fade-right">
                <div class="card-body">
                    <div>
                        <input type="text" id="range" value="" name="range" />
                        <label class="text-muted small">Income</label>

                    </div>
                </div>
            </div>
        </div>
        <div class="container pt-2" data-aos="fade-right">
            <div class="card">
                <div class="card-body">
                    <div>
                        <input type="text" id="range3" value="" name="range" />
                        <label class="text-muted small">Age Range</label>

                    </div>
                </div>
            </div>
        </div>
        <div class="container pt-2" data-aos="fade-right">
            <div class="card">
                <div class="card-body">
                    <div>
                        <input type="text" id="range4" value="" name="range" />
                        <label class="text-muted small">Household</label>

                    </div>
                </div>
            </div>
        </div>
    </aside>
    <aside class="aside aside-2" data-aos="fade-left">
        <h4>Challenges</h4>
        <p class="small">
            Join us for one of our weekly challenges. Earn achievements and compare yourself to your
            peers.
        </p>
        <p class="small"><a href="#">View current challenges</a></p>
        <hr>
        <h4>Peers</h4>
        <span class="">Tony B.</span><br>
        <span class="small">saving for retirement</span><br>
        <span class="small">Debt down to 43%.</span><br>
        <span class="small"><a href="#">Follow</a></span><br>
        <hr>
        <span class="">Tony B.</span><br>
